@model IEnumerable<PracticaInforTec1.Models.PersonWithReward>
@{
    ViewBag.Title = "PeopleWithRewards";
}

<h2>People with rewards</h2>


<table class="table" id="person_with_rewards_ul">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.personId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.personName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.rewardName)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.personId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.personName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.rewardName)
            </td>
        </tr>
    }

</table>

